<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Object Save Mode - control of switching ON/OFF validations, processes, workflows and Trigger logic for given user who will run data fix.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>DisableValidation__c</fullName>
        <defaultValue>false</defaultValue>
        <description>No validations will be executed when field is set to True</description>
        <label>Disable Validation</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Disable_Process_Builder__c</fullName>
        <defaultValue>false</defaultValue>
        <description>No Process Builder will be executed when field is set to True</description>
        <label>Disable Process Builder</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Disable_Trigger__c</fullName>
        <defaultValue>false</defaultValue>
        <description>No Tiggers will be executed when field is set to True</description>
        <label>Disable Trigger</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Disable_Workflow__c</fullName>
        <defaultValue>false</defaultValue>
        <description>No Workflow will be executed when field is set to True</description>
        <label>Disable Workflow</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Full_Logic_Disabled__c</fullName>
        <defaultValue>false</defaultValue>
        <description>No validations, processes, workflows and triggers will be executed when field is set to True</description>
        <label>Full Logic Disabled</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Object Save Mode</label>
    <visibility>Public</visibility>
</CustomObject>
