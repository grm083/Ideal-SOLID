<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>The configurable elements of the Add/Change Add Quote functionality.  Will be used to determine whether or not the Add Quote button should be displayed.  Will remove the add quote button from scope for non-New Service requests and will instead layer the functionality onto the Master Intake Flow</description>
    <fields>
        <fullName>Case_Reason__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Case Reason</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Case_Sub_Type__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Case Sub-Type</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Case_Type__c</fullName>
        <description>Provides Case Type as a text box that allows us to track which case types are allowed through the Add/Change quote process</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Case Type</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Restriction_Type__c</fullName>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Restriction Type</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Case Classification (Type, Sub-Type, Reason)</fullName>
                    <default>true</default>
                    <label>Case Classification (Type, Sub-Type, Reason)</label>
                </value>
                <value>
                    <fullName>User Categorization Code</fullName>
                    <default>false</default>
                    <label>User Categorization Code</label>
                </value>
                <value>
                    <fullName>Price Model Type</fullName>
                    <default>false</default>
                    <label>Price Model Type</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <label>Add-Change Scope Controller</label>
    <listViews>
        <fullName>Restrictions_View</fullName>
        <columns>MasterLabel</columns>
        <columns>DeveloperName</columns>
        <columns>Restriction_Type__c</columns>
        <columns>Case_Type__c</columns>
        <columns>Case_Sub_Type__c</columns>
        <columns>Case_Reason__c</columns>
        <filterScope>Everything</filterScope>
        <label>Restrictions View</label>
    </listViews>
    <pluralLabel>Add-Change Scope Controllers</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
