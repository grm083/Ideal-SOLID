    <validationRules>
        <fullName>Validate_BRsApprover</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISBLANK(  New_Contact_Approver__c  ),
ISBLANK(   New_Email_Approver__c   ),
ISBLANK(   New_User_Approver__c  )
)</errorConditionFormula>
        <errorMessage>Please Enter an Approver to Continue</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_EndDateAndStartDate</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c &lt;  Start_Date__c</errorConditionFormula>
        <errorMessage>End Date must be greater than or equal to start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_MorethanOneApprovers</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(NOT(ISBLANK(New_Contact_Approver__c)),NOT(ISBLANK(New_User_Approver__c)),NOT(ISBLANK(New_Email_Approver__c)) 
),
AND(NOT(ISBLANK(New_Contact_Approver__c)),NOT(ISBLANK(New_User_Approver__c))
),
AND(NOT(ISBLANK(New_Contact_Approver__c)),NOT(ISBLANK(New_Email_Approver__c))
),
AND(NOT(ISBLANK(New_Email_Approver__c)),NOT(ISBLANK(New_User_Approver__c))
)
)</errorConditionFormula>
        <errorMessage>Please populate only one form of New Approver</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_SameApprover</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
AND(NOT(ISBLANK(Current_User_Approver__c)),NOT(ISBLANK(New_User_Approver__c)),Current_User_Approver__c=New_User_Approver__c),
AND(NOT(ISBLANK(Current_User_Approver__r.Email)),NOT(ISBLANK(New_Email_Approver__c)),Current_User_Approver__r.Email=New_Email_Approver__c),
AND(NOT(ISBLANK(Current_User_Approver__r.Email)),NOT(ISBLANK(New_Contact_Approver__r.Email)),Current_User_Approver__r.Email=New_Contact_Approver__r.Email),
AND(NOT(ISBLANK(Current_Email_Approver__c)),NOT(ISBLANK(New_User_Approver__r.Email)),Current_Email_Approver__c=New_User_Approver__r.Email),
AND(NOT(ISBLANK(Current_Email_Approver__c)),NOT(ISBLANK(New_Email_Approver__c)),Current_Email_Approver__c=New_Email_Approver__c),
AND(NOT(ISBLANK(Current_Email_Approver__c)),NOT(ISBLANK(New_Contact_Approver__r.Email)),Current_Email_Approver__c=New_Contact_Approver__r.Email),
AND(NOT(ISBLANK(Current_Contact_Approver__r.Email)),NOT(ISBLANK(New_User_Approver__r.Email)),Current_Contact_Approver__r.Email=New_User_Approver__r.Email),
AND(NOT(ISBLANK(Current_Contact_Approver__r.Email)),NOT(ISBLANK(New_Email_Approver__c)),Current_Contact_Approver__r.Email=New_Email_Approver__c),
AND(NOT(ISBLANK(Current_Contact_Approver__r.Email)),NOT(ISBLANK(New_Contact_Approver__r.Email)),Current_Contact_Approver__r.Email=New_Contact_Approver__r.Email)
)</errorConditionFormula>
        <errorMessage>The User selected cannot be the existing approver and the new approver for this business rule, please select a different user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_StartDate</fullName>
        <active>true</active>
        <errorConditionFormula>OR( AND(ISNEW() &amp;&amp; Start_Date__c &lt; TODAY()),
    AND(  ISCHANGED(Start_Date__c) &amp;&amp;  Start_Date__c  &lt;  PRIORVALUE(Start_Date__c ) )
)</errorConditionFormula>
        <errorMessage>Start Date should be today or Future Date.</errorMessage>
    </validationRules>
