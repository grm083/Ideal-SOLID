        <errorConditionFormula>AND(Return_DateTime__c &lt; Pickup_DateTime__c,
NOT($User.Bypass_Validation__c))</errorConditionFormula>
        <errorDisplayField>Return_DateTime__c</errorDisplayField>
        <errorMessage>Return Date/Time must be &gt;= Pickup Date/Time</errorMessage>
    </validationRules>
