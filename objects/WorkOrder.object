    <validationRules>
        <fullName>Service_Date_Validation</fullName>
        <active>true</active>
        <description>SDT- 4113
Service Date can be changed only if WO Status IN (New, Pending/Draft, Issued, Sent)</description>
        <errorConditionFormula>AND(ISCHANGED( Service_Date__c ),NOT(ISBLANK(TEXT(Status))),
NOT(  
OR(ISPICKVAL( Status , &quot;New&quot;),
ISPICKVAL( Status , &quot;Pending/Draft&quot;),
ISPICKVAL( Status , &quot;Issued&quot;),
ISPICKVAL( Status , &quot;Sent&quot;),ISPICKVAL( Status , &quot;Rejected&quot;))),
NOT($User.Bypass_Validation__c)
)</errorConditionFormula>
        <errorMessage>Service Date can be changed only if WO Status IN (New, Pending/Draft, Issued, Sent,Rejected)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_Dates_Validation_Actual_Service</fullName>
        <active>true</active>
        <description>SDT 4113 story. Update date validation criteria.</description>
        <errorConditionFormula>AND(DATEVALUE(Actual_Service_Date__c )&lt; Service_Date__c,
NOT($User.Bypass_Validation__c))</errorConditionFormula>
        <errorDisplayField>Actual_Service_Date__c</errorDisplayField>
        <errorMessage>Actual Service Date/Time must be &gt;= Service Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_Dates_Validation_Return</fullName>
        <active>true</active>
