    <validationRules>
        <fullName>Canada_Customer_Not_Null_When_All_False</fullName>
        <active>true</active>
        <description>Canada Customer should not be blank in case All is Unchecked</description>
        <errorConditionFormula>AND( Customer_All_Canada__c  = false,  Customer_Canada__c  = null,  RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>If &quot;All Canadian Customers&quot; is unchecked, then Listed Customer(s) must be provided.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Canada_Customer_not_null_and_All_checked</fullName>
        <active>true</active>
        <description>Canada Customer should not be blank in case All is Unchecked</description>
        <errorConditionFormula>AND( Customer_All_Canada__c  = true,  Customer_Canada__c  != null,  RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>If All Canadian Customers is checked, then Canada Listed Customers must be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Not_Null_When_All_is_False</fullName>
        <active>true</active>
        <description>Customer should not be blank in case All is Unchecked</description>
        <errorConditionFormula>AND( Customer_All__c  = false,  Customer__c  = null,  RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>If &quot;All US Customers&quot; is unchecked, then Listed Customer(s) must be provided.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Customer_Null_When_All_is_True</fullName>
        <active>true</active>
        <description>Customer should not be blank in case All is Unchecked</description>
        <errorConditionFormula>AND( Customer_All__c  = true,  Customer__c  != null,  RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>If All US Customers is checked, then US Listed Customers must be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exclude_All_Validation</fullName>
        <active>true</active>
        <description>Prevent Records From Having to Exclude All Customers</description>
        <errorConditionFormula>AND(Customer_All__c  = true,  Customer_Included__c  = false, Customer_All_Canada__c = true, Customer_Included_Canada__c = false, RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>All US and all Canadian customers cannot be excluded.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exclude_All_Vendor_Validation</fullName>
        <active>true</active>
        <description>Prevent Records From Having to Exclude All Vendors</description>
        <errorConditionFormula>AND(All_WM_Vendors__c  = true,  WM_Vendor__c= false, All_3rd_Party_Vendor__c = true, Child_VID_Check_Include_Uncheck_Exclude__c = false, RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>All WM and 3rd Party vendors cannot be excluded.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exclude_Canada_All_Validation</fullName>
        <active>false</active>
        <description>Prevent Canadian Records From Having to Exclude All Customers</description>
        <errorConditionFormula>AND(Customer_All_Canada__c  = true,  Customer_Included_Canada__c  = false, RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>When excluding Canadian customers, only listed customers, not all customers, can be selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Market_area_is_required</fullName>
        <active>true</active>
        <description>When selecting WM vendors market area is required.</description>
        <errorConditionFormula>AND(
OR( 
AND((WM_Vendor__c),(All_WM_Vendors__c) , ISBLANK( Parent_VID__c)), 
AND(NOT((All_WM_Vendors__c)) , NOT(ISBLANK( Parent_VID__c )))
),
 (ISNULL( MAS__c ))
)</errorConditionFormula>
        <errorMessage>Please select market area.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ParentVendor_Not_Null_When_All_is_False</fullName>
        <active>true</active>
        <description>Parent Vendor should not be blank in case All WM Parent Vendors is Unchecked</description>
        <errorConditionFormula>AND( All_WM_Vendors__c  = false,  Parent_VID__c  = null,  RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>If &quot;All WM Parent Vendors&quot; is unchecked, then Listed WM Parent VID(s) must be provided.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Vendor3P_Not_Null_When_All_False</fullName>
        <active>true</active>
        <description>Third Party Vendor should not be blank in case All is Unchecked</description>
        <errorConditionFormula>AND(All_3rd_Party_Vendor__c = false,  Parent_Non_Parent_VID_s__c = null,  RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>If &quot;All 3P Parent &amp; Non-Parent Vendors&quot; is unchecked, then Listed 3P Listed Parent &amp; Non-Parent Vendors must be provided.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Vendor3P_not_null_and_All_checked</fullName>
        <active>true</active>
        <description>Third Party Vendor should not be blank in case All is Unchecked</description>
        <errorConditionFormula>AND( All_3rd_Party_Vendor__c = true,  Parent_Non_Parent_VID_s__c  != null,  RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>If All Third Party Vendors is checked, then Listed 3rd Party Vendors must be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WMParentVendor_Null_When_All_is_True</fullName>
        <active>true</active>
        <description>WM Parent Vendor should not be blank in case All is Unchecked</description>
        <errorConditionFormula>AND( All_WM_Vendors__c  = true,  Parent_VID__c  != null,  RecordType.Name = &apos;STP Eligibility&apos;)</errorConditionFormula>
        <errorMessage>If All WM Parent Vendor is checked, then Listed WM Parent Vendor must be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WM_Market_Area_Selection</fullName>
        <active>true</active>
        <description>WM Market Area must be selected when WM Vendor is selected</description>
        <errorConditionFormula>AND( RecordType.Name = &apos;STP Eligibility&apos;,
 NOT(ISNULL(MAS__c)) , WM_Vendor__c = false,  All_WM_Vendors__c  = true,  Parent_VID__c  = null )</errorConditionFormula>
        <errorMessage>WM Market Area cannot be selected without selecting WM Vendor</errorMessage>
    </validationRules>
