    <fields>
        <fullName>Contact_Name__c</fullName>
        <description>Lookup for Contact</description>
        <label>Contact Name</label>
        <referenceTo>Contact</referenceTo>
        <relationshipLabel>Contact Notification Preferences</relationshipLabel>
        <relationshipName>Contact_Notification_Preferences</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Notification_Preference__c</fullName>
        <description>Field to hold notification preference</description>
        <label>Notification Preference</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>MultiselectPicklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Do Not Notify</fullName>
                    <default>false</default>
                    <label>Do Not Notify</label>
                </value>
                <value>
                    <fullName>Email</fullName>
                    <default>false</default>
                    <label>Email</label>
                </value>
                <value>
                    <fullName>SMS</fullName>
                    <default>false</default>
                    <label>SMS</label>
                </value>
            </valueSetDefinition>
        </valueSet>
        <visibleLines>4</visibleLines>
    </fields>
    <fields>
        <fullName>Notification_Type__c</fullName>
        <description>Picklist to hold notification types</description>
        <label>Notification Type</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetName>Notification_Type</valueSetName>
        </valueSet>
    </fields>
    <fields>
        <fullName>Preferred_Language__c</fullName>
        <description>Field to hold preferred language for notification</description>
        <label>Preferred Language</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>English</fullName>
                    <default>false</default>
                    <label>English</label>
                </value>
                <value>
                    <fullName>French (Francais)</fullName>
                    <default>false</default>
                    <label>French (Francais)</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <label>Contact Notification Preference</label>
    <nameField>
        <displayFormat>CNP-{0000000}</displayFormat>
        <label>Contact Notification Preference Name</label>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Contact Notification Preferences</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Do_not_notify_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
INCLUDES(Notification_Preference__c , &quot;Do Not Notify&quot;), 
OR(
INCLUDES(Notification_Preference__c , &quot;SMS&quot;), 
INCLUDES(Notification_Preference__c , &quot;Email&quot;)
) 
)</errorConditionFormula>
        <errorMessage>You can not select any other notification preference with Do Not Notify</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
